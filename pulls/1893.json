      "message": "scripted-diff: fix BIP 2 field order violations\n\n-BEGIN VERIFY SCRIPT-\nset -e\nperl <<'-END PERL-'\nuse strict;\nuse warnings;\n\nmy $topbip = 9999;\n\nmy @FieldOrder = qw(\n\tBIP\n\tLayer\n\tTitle\n\tAuthor\n\tAuthors\n\tEditor\n\tDeputies\n\tDiscussions-To\n\tComments-Summary\n\tComments-URI\n\tStatus\n\tType\n\tCreated\n\tLicense\n\tLicense-Code\n\tDiscussion\n\tPost-History\n\tVersion\n\tRequires\n\tReplaces\n\tProposed-Replacement\n\tSuperseded-By\n);\n\nmy $bipnum = 0;\nwhile (++$bipnum <= $topbip) {\n\tmy $fn = sprintf \"bip-%04d.mediawiki\", $bipnum;\n\tmy $is_markdown = 0;\n\tif (!-e $fn) {\n\t\t$fn = sprintf \"bip-%04d.md\", $bipnum;\n\t\t$is_markdown = 1;\n\t}\n\t-e $fn || next;\n\topen my $F, \"<\", $fn or die \"$!\";\n\n\tmy (@before, %preamble, @after);\n\n\tif ($is_markdown) {\n\t\twhile (<$F>) {\n\t\t\tpush @before, $_;\n\t\t\tlast if m[^(?:\\xef\\xbb\\xbf)?```$]\n\t\t}\n\t\tdie \"No ``` in $fn\" if eof $F;\n\t} else {\n\t\twhile (<$F>) {\n\t\t\tpush @before, $_;\n\t\t\tlast if m[^(?:\\xef\\xbb\\xbf)?<pre>$];\n\t\t}\n\t\tdie \"No <pre> in $fn\" if eof $F;\n\t}\n\tmy %found;\n\tmy ($title, $author, $status, $type, $layer);\n\tmy ($field, $val, @field_order);\n\twhile (<$F>) {\n\t\tpush @after, $_ and last if ($is_markdown && m[^```$]);\n\t\tpush @after, $_ and last if (!$is_markdown && m[^</pre>$]);\n\n\t\tif (m[^  ([\\w-]+)\\: (.*\\S)$]) {\n\t\t\t$field = $1;\n\t\t\t$val = $2;\n\t\t} elsif (m[^  ( +)(.*\\S)$]) {\n\t\t\t$val = $2;\n\t\t} else {\n\t\t\tdie \"Bad line in $fn preamble\";\n\t\t}\n\n\t\tpush @{$preamble{$field} ||= []}, $_;\n\t}\n\tpush @after, <$F>;\n\tclose $F or die $!;\n\n\topen my $W, \">\", \"$fn\" or die \"$!\";\n\n\tprint $W @before;\n\tprint $W map { @$_ } grep { defined } delete @preamble{@FieldOrder};\n\tdie \"Unknown fields: @{[ keys %preamble ]}\" if %preamble;\n\tprint $W @after;\n\n\tclose $W or die $!;\n}\n-END PERL-\n-END VERIFY SCRIPT-",
