      "starred_url": "https://api.github.com/users/yyhrnk/starred%7B/owner%7D%7B/repo%7D",
      "subscriptions_url": "https://api.github.com/users/yyhrnk/subscriptions",
      "organizations_url": "https://api.github.com/users/yyhrnk/orgs",
      "repos_url": "https://api.github.com/users/yyhrnk/repos",
