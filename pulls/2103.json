      "subscriptions_url": "https://api.github.com/users/jasonandjay/subscriptions",
      "organizations_url": "https://api.github.com/users/jasonandjay/orgs",
      "repos_url": "https://api.github.com/users/jasonandjay/repos",
      "events_url": "https://api.github.com/users/jasonandjay/events%7B/privacy%7D",
      "received_events_url": "https://api.github.com/users/jasonandjay/received_events",
