      "tree": {
        "sha": "e4e74837b754cbc85f73f705bb1917308481e4f0",
        "url": "https://api.github.com/repos/bitcoin/bips/git/trees/e4e74837b754cbc85f73f705bb1917308481e4f0"
      },
      "verification": {
        "verified": true,
